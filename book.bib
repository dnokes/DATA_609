@article{Fenn2011-yl,
  title       = "Temporal evolution of financial-market correlations",
  author      = "Fenn, Daniel J and Porter, Mason A and Williams, Stacy and
                 McDonald, Mark and Johnson, Neil F and Jones, Nick S",
  affiliation = "Mathematical and Computational Finance Group, Mathematical
                 Institute, University of Oxford, Oxford OX1 3LB, United
                 Kingdom.",
  abstract    = "We investigate financial market correlations using random
                 matrix theory and principal component analysis. We use random
                 matrix theory to demonstrate that correlation matrices of
                 asset price changes contain structure that is incompatible
                 with uncorrelated random price changes. We then identify the
                 principal components of these correlation matrices and
                 demonstrate that a small number of components accounts for a
                 large proportion of the variability of the markets that we
                 consider. We characterize the time-evolving relationships
                 between the different assets by investigating the correlations
                 between the asset price time series and principal components.
                 Using this approach, we uncover notable changes that occurred
                 in financial markets and identify the assets that were
                 significantly affected by these changes. We show in particular
                 that there was an increase in the strength of the
                 relationships between several different markets following the
                 2007-2008 credit and liquidity crisis.",
  journal     = "Phys. Rev. E Stat. Nonlin. Soft Matter Phys.",
  volume      =  84,
  number      = "2 Pt 2",
  pages       = "026109",
  month       =  aug,
  year        =  2011,
  language    = "en"
}
